{
    "Debug": true,
    "AppName": "Job Scheduler",
    "AppRoot": "/opt/app/scheduler",
    "Service": {
        "Client": {
            "Name": "client",
            "Logs": {
                "Path": "client/logs",
                "Prefix": "schd_cli_",
                "DateFormat": "yyyymmdd",
                "Extension": "log"
            }
        },
        "EventServer": {
            "Name": "eventserver",
            "Port": 12005,
            "Logs": {
                "Path": "eventserver/logs",
                "Prefix": "schd_eserver_",
                "DateFormat": "yyyymmdd",
                "Extension": "log"
            }
        },
        "Allocator": {
            "Name": "allocator",
            "Port": 12006,
            "Logs": {
                "Path": "allocator/logs",
                "Prefix": "schd_alloc_",
                "DateFormat": "yyyymmdd",
                "Extension": "log"
            }
        },
        "Controller": {
            "Name": "controller",
            "Port": 12007,
            "Logs": {
                "Path": "controller/logs",
                "Prefix": "schd_ctrl_",
                "DateFormat": "yyyymmdd",
                "Extension": "log"
            }
        },
        "Validator": {
            "Name": "validator",
            "Port": 12008,
            "Logs": {
                "Path": "validator/logs",
                "Prefix": "schd_val_",
                "DateFormat": "yyyymmdd",
                "Extension": "log"
            }
        }
    }
}
